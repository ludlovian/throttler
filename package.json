{
  "name": "throttler",
  "version": "1.1.3",
  "description": "Rate limiting transform stream",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist/ node_modules/.cache/ .nyc_output/ coverage/",
    "format": "prettier-standard src/*.js test/*.js",
    "lint": "standard src/*.js test/*.js | snazzy",
    "test": "nyc ava",
    "coverage": "npm run test && nyc report --reporter=html && sirv coverage -H '0.0.0.0' -p 8000",
    "build": "rollup -c rollup.config.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ava": "^3.10.1",
    "esm": "^3.2.25",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-terser": "^6.1.0"
  },
  "ava": {
    "require": [
      "esm"
    ]
  }
}
